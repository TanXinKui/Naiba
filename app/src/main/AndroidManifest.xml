<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="www.xinkui.com.odering">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:name=".network.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".welcome.Introduction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.Login"></activity>
        <activity android:name=".bean.ComInf"></activity>
        <activity android:name=".user.Register"></activity>
        <activity android:name=".activity.Main"></activity>
        <activity android:name=".activity.OrderList"></activity>
        <activity android:name=".activity.ClientInfo"></activity>
        <activity android:name=".activity.SearchHistory"></activity>
        <activity android:name=".activity.CommentList"></activity>

        <service
            android:name=".service.SqlQuery"
            android:exported="false"></service>

    </application>

</manifest>